<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="calendar">
        <attr name="week_height" format="dimension"/>
        <attr name="schedule_height" format="dimension"/>
        <attr name="min_scheduleHeight" format="dimension"/>
        <attr name="indicator_height" format="dimension"/>
        <attr name="indicator_layout" format="reference"></attr>
        <attr name="calendar_type">
            <enum name="week" value="0"></enum>
            <enum name="month" value="1"></enum>
            <enum name="schedule" value="2"></enum>
        </attr>
        <attr name="calendar_clickable" format="boolean"/>
    </declare-styleable>

</resources>